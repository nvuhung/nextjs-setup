    "full-icu": "^1.2.1",
    "glob": "^7.1.3",
    "graphql": "^14.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "less": "^3.9.0",
    "lodash": "^4.17.11",
    "next": "^7.0.2",
    "next-i18next": "^0.4.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
