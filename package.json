{
  "private": true,
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "rimraf .next && next build src && rimraf build && tsc --project tsconfig.server.json && ln -s ../src/static build/static",
    "start": "NODE_ENV=production node build/server.js",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "lint": "tsc --noEmit && tslint -c tslint.json 'src/**/*.ts{x,}' && prettier-check --ignore-path .gitignore \"{.,src/**}/{*.ts{x,},*.md,ts*.json,*.config.js}\"",
    "format": "prettier --write --ignore-path .gitignore \"{.,src/**}/{*.ts{x,},*.md,ts*.json,*.config.js}\"",
  },
  "dependencies": {
