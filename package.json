    "@zeit/next-less": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-styled-components": "^1.9.1",
    "next-compose-plugins": "^2.1.1",
    "nodemon": "^1.18.7",
